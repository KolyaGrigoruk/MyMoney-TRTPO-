<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usr.mymoney">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.IncomeActivity"
            android:label="@string/title_activity_finance"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.StatisticActivity"
            android:label="@string/title_activity_statistic"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.SpendingActivity"
            android:label="@string/title_activity_costs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.PlaningActivity"
            android:label="@string/title_activity_plans"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.InfoActivity"
            android:label="@string/title_activity_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EditingIncomeActivity"
            android:label="@string/title_activity_editing_income"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EditingSpendingActivity"
            android:label="@string/title_activity_editing_spending"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EditingFinanceObjActivity"
            android:label="@string/title_activity_editinig_finance_obj"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EditingOneObjActivity"
            android:label="@string/title_activity_editing_one_obj"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>